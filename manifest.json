{
  "manifest_version": 2,

  "name": "Pipefy Harvest Extension",
  "description": "This extension integrates pipefy and harvest",
  "version": "1.3",

  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [ "*://app.pipefy.com/*" ],
      "js": [ "pipefy.js" ],
      "run_at": "document_start"
    }
  ]
}
